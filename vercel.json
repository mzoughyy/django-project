{
    "version":2,
    "builds":[
        {
        "src": "Syndic/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"1go","runtime":"python3.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{"distDir":"static_files_build"}
    
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest":"/Syndic/wsgi.py"
        }
    ]
}